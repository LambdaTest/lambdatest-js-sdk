{
  "name": "appium-navigation-tracker",
  "version": "1.0.0",
  "description": "A package to track user screen navigations in mobile apps using Appium",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "appium",
    "mobile",
    "testing",
    "navigation",
    "tracking",
    "lambdatest"
  ],
  "author": "LambdaTest",
  "license": "MIT",
  "dependencies": {
    "@lambdatest/sdk-utils": "workspace:^",
    "@types/node": "^18.0.0",
    "appium": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tslint": "^6.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/__tests__/**/*"
    ]
  }
}
